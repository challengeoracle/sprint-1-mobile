{
    "expo": {
        "name": "medix",
        "slug": "medix",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/medix-logo.png",
        "scheme": "medix",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/images/medix-logo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "softwareKeyboardLayoutMode": "pan",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/medix-logo.png",
                "backgroundColor": "#ffffff"
            },
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/medix-logo.png"
        },
        "plugins": ["expo-router", "expo-web-browser"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
